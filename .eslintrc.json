{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "react/prop-types": [0],
    "react/no-multi-comp": [0],
    "no-unused-vars": [1],
    "class-methods-use-this": [0],
    "global-require": [0],
    "no-await-in-loop": [0],
    "no-loop-func": [0],
    "no-continue": [0],
    "no-lonely-if": [0]
  }
}
